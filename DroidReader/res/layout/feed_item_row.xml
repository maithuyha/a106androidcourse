<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  	xmlns:android="http://schemas.android.com/apk/res/android"
  	android:layout_width="fill_parent"
  	android:orientation="vertical"
  	android:layout_height="wrap_content" android:padding="5px">
    
	<TextView 
		android:id="@+id/feedItemTitleLabel" 
		android:text="Title"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" android:textStyle="bold" android:textSize="22dp">
	</TextView>
	
	<TextView 
		android:id="@+id/feedItemPudDateLabel" 
		android:text="Pubdate"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content">
	</TextView>
    
</LinearLayout>
